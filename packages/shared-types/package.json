  "author": "jackdo69",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jackdo69/job-tracker.git",
    "directory": "packages/shared-types"
  }
