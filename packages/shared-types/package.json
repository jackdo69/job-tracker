{
  "name": "@jackdo69/job-tracker-shared-types",
  "version": "2.0.0",
  "description": "Shared TypeScript types for Job Tracker application",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
